{
  "version": 2,
  "name": "atlas-portfolio",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "alias": ["atlas.vercel.app"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}